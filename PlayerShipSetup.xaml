<Window x:Class="SchiffeVersenken.PlayerShipSetup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SchiffeVersenken"
        mc:Ignorable="d"
        Title="Player Ship Setup" Height="430" Width="750">
    <Grid Name="Grid">
        <Button Name="MainMenue" Content="Back to Menue" HorizontalAlignment="Right" Height="20" Margin="0,0,10,10" VerticalAlignment="Bottom" Width="90" Click="MainMenue_Click"/>
        <Button Name="BTN_Up" Content="Nord" HorizontalAlignment="Center" Height="50" Margin="0,0,0,111" VerticalAlignment="Bottom" Width="50" Click="BTN_Up_Click"/>
        <Button Name="BTN_Down" Content="Süd" HorizontalAlignment="Center" Height="50" Margin="0,0,0,9" VerticalAlignment="Bottom" Width="50" Click="BTN_Down_Click"/>
        <Button Name="BTN_Left" Content="West" HorizontalAlignment="Center" Height="50" Margin="0,0,101,60" VerticalAlignment="Bottom" Width="50" Click="BTN_Left_Click"/>
        <Button Name="BTN_Right" Content="Ost" HorizontalAlignment="Center" Height="50" Margin="0,0,-101,60" VerticalAlignment="Bottom" Width="50" Click="BTN_Right_Click"/>
    </Grid>
</Window>
